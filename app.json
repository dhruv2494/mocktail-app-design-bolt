{
  "expo": {
    "name": "Mocktale Academy",
    "slug": "Mocktale Academy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MockTale.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/MockTale.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-camera",
      "expo-splash-screen"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0c6ce86e-f047-4e02-9de7-cee1b0146cff"
      }
    },
    "android": {
      "package": "com.hello2494.boltexponativewind",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "owner": "wegacano"
  }
}
